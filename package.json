		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.1",
		"vaul-svelte": "1.0.0-next.3",
		"vite": "^6.0.9",
		"vite-imagetools": "^7.0.5",
		"vite-plugin-mkcert": "^1.17.6",
		"vitest": "^2.1.8"
