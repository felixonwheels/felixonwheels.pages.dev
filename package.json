		"@tailwindcss/typography": "^0.5.16",
		"bits-ui": "1.0.0-next.74",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"lucide-svelte": "^0.474.0",
		"prettier": "^3.4.2",
